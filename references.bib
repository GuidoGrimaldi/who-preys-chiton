@article{liu2022b,
  title = {Optimized {{Sensory Units Integrated}} in the {{Chiton Shell}}},
  author = {Liu, Chuang and Liu, Haipeng and Huang, Jingliang and Ji, Xin},
  year = {2022},
  month = mar,
  journal = {Marine Biotechnology},
  volume = {24},
  pages = {380--392},
  issn = {1436-2236},
  doi = {10.1007/s10126-022-10114-2},
  urldate = {2022-03-15},
  abstract = {The first step for animals to interact with external environment is to sense. Unlike vertebrate animals with flexibility, it is challenging for ancient animals that are less flexible especially for mollusca with heavy shells. Chiton, as an example, has eight overlapping shells covering almost the whole body, is known to incorporate sensory units called aesthetes inside the shell. We used micro-computed tomography combined with quantitative image analysis to reveal the optimized shell geometry to resist force and the aesthetes' global distribution at the whole animal levels to facilitate sense from diverse directions both in the seawater and air. Additionally, shell proteomics combined with transcriptome reveals shell matrix proteins responsible for shell construction and potentially sensory function, highlighting unique cadherin-related proteins among mollusca. Together, this multi-level evidence of sensory units in the chiton shell may shed light on the formation of chiton shells and inspire the design of hard armor with sensory function.},
  langid = {english},
  keywords = {Animal Eyes,Animal Vision,Biomaterials,Biomineralization,Chitons,Invertebrates,LIDO,Marine,Sensorial},
  file = {C:\Users\Guido\Zotero\storage\956I6FMY\2022_Liu etal_Optimized Sensory Units Integrate.pdf}
