@article{reyment1967,
  title = {Paleoethology and {{Fossil Drilling Gastropods}}},
  author = {Reyment, Richard A.},
  year = {1967},
  journal = {Transactions of the Kansas Academy of Science},
  volume = {70},
  number = {1},
  eprint = {3627612},
  eprinttype = {jstor},
  pages = {33--50},
  publisher = {Kansas Academy of Science},
  issn = {0022-8443},
  doi = {10.2307/3627612},
  urldate = {2024-12-27},
  abstract = {Gastropods of the Muricidae and Naticidae are active predators of pelecypods, gastropods, scaphopods and ostracodes. Naticids seek their prey in the upper layers of the bottom sediment, whereas muricids search for theirs on the surface of the sediment. Naticids only attack living organisms, whereas muricids eat both living animals and carrion. The holes drilled by muricids are mostly smaller than naticid holes and they are more straight-sided. The identification of the group of drills that gave rise to holes in fossil shells is considered a useful criterion in analyzing a micro-sedimentary environment. The concept of paleoethology is defined, in the present connection, as interpreting the attack behavior of drills based on an analysis of the location and nature of drill holes. The present study treats Paleogene ostracodes from subsurface samples in Western Nigeria and living ostracodes and mollusks from the western Niger Delta. By statistical analysis of the hole dimensions it is usually possible to identify the group of predaceous gastropods responsible for the drilling. Valve preference may be shown by naticids in drilling pelecypods but apparently not in drilling ostracodes; this may be connected with the differing mobility of the two groups. An analysis of hole shape is made.},
  langid = {english},
  keywords = {Drill hole,Drilling,Gastropods,Molluscs,Paleontology}
}

